<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <version>$version$</version>
    <authors>Matt Wrock</authors>
    <owners>Matt Wrock</owners>
    <projectUrl>http://Boxstarter.org</projectUrl>
    <id>Boxstarter.Common</id>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <iconUrl>http://download-codeplex.sec.s-msft.com/Download?ProjectName=boxstarter&amp;DownloadId=635599&amp;Build=20006</iconUrl>
    <title>Boxstarter Common Module</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Provides common functions used by multiple Boxstarter Modules.</description>
    <releaseNotes>
      - Can supply multiple packages to Install-BoxstarterPackage and Launch URLs
      - Boxstarter.Azure now globally imports the Windows Azure module
      - Fixed issues with using the Boxstarter Web launcher to install Boxstarter
      - Fixed errors when installing Boxstarter with Chocolatey in the same session after installing Chocolatey
      - Adds Windows Azure VM Integration
      - Supports VM Checkpoints using Azure Blob Snapshots
      - Adds Graceful failure to Enable-RemoteDesktop for Home versions of Windows 7
      - Adds support for unattended install of Boxstarter
    </releaseNotes>
  </metadata>
  <files>
    <file src="..\..\LICENSE.txt" target="tools"/>
    <file src="..\chocolateyinstall.ps1" target="tools"/>
    <file src="..\Setup.ps1" target="tools"/>    
    <file src="..\..\Boxstarter.Common\**\*.*" target="tools\Boxstarter.Common"/>
  </files>
</package>